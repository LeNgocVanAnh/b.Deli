<!-- Booking Area Start -->
<div scroll-spy="true" id="reservation" class="booking-block section-padding">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="section-heading">
                    <h1>Đặt Bàn</h1>
                    <div class="section-border"></div>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-4 col-lg-3">
                <div class="booking-info">
                    <h5>Thời gian mở cửa</h5>
                    <table class="table table-borderless">
                        <tbody>
                            <tr>
                                <td><strong>Thứ Hai</strong></td>
                                <td>10am - 11pm</td>
                            </tr>
                            <tr>
                                <td><strong>Thứ Ba</strong></td>
                                <td>10am - 11pm</td>
                            </tr>
                            <tr>
                                <td><strong>Thứ Tư</strong></td>
                                <td>10am - 11pm</td>
                            </tr>
                            <tr>
                                <td><strong>Thứ Năm</strong></td>
                                <td>10am - 11pm</td>
                            </tr>
                            <tr>
                                <td><strong>Thứ Sáu</strong></td>
                                <td>10am - 11pm</td>
                            </tr>
                            <tr>
                                <td><strong>Thứ Bảy</strong></td>
                                <td>9am - 12am</td>
                            </tr>
                            <tr>
                                <td><strong>Chủ Nhật</strong></td>
                                <td>9am - 12am</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>

            <div class="col-md-8 col-lg-9">
                <div class="booking-form">
                    <h5>Đặt trước</h5>
                    <form action="/home/save" method="post">
                        <div class="row">
                            <div class="col-md-6">
                                <input id="client-name" name="name" type="text" placeholder="Họ và Tên" required="required">
                                <input id="client-email" name="email" type="email" placeholder="Email" required="required">
                                <input id="booking-date" name="date" type="text" placeholder="Ngày đặt" required="required">
                                <input id="booking-time" name="time" type="text" placeholder="Thời gian đặt" required="required">
                            </div>
                            <div class="col-md-6">
                                <input id="guest" name="guest" type="text" placeholder="Số lượng khách" required="required">
                                <textarea name="message" id="client-message" cols="30" rows="10" placeholder="Lời nhắn"></textarea>
                                <button class="btn cafetora-fullwidth-btn" type="submit">Đặt bàn ngay!</button>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-response"></div>
                            </div>
                        </div>

                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Booking Area End -->
